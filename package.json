{
  "name": "mh-tools",
  "version": "1.0.0",
  "description": "Assorted Mousehunt Tools",
  "scripts": {
    "puppeteer": "npm i -S puppeteer",
    "py": "py -m SimpleHTTPServer",
    "bookmarklet:cre": "uglifyjs src/bookmarklet/crebookmarklet.js -cmo src/bookmarklet/crebookmarklet.min.js",
    "bookmarklet:setup": "uglifyjs src/bookmarklet/setupbookmarklet.js -cmo src/bookmarklet/setupbookmarklet.min.js",
    "bookmarklet:map": "uglifyjs src/bookmarklet/mapbookmarklet.js -cmo src/bookmarklet/mapbookmarklet.min.js",
    "bookmarklet:crown": "uglifyjs src/bookmarklet/crownbookmarklet.js -cmo src/bookmarklet/crownbookmarklet.min.js",
    "bookmarklet:analyzer": "uglifyjs src/bookmarklet/analyzerbookmarklet.js -cmo src/bookmarklet/analyzerbookmarklet.min.js",
    "bookmarklet:loader": "uglifyjs src/bookmarklet/bookmarkletloader.js -cmo src/bookmarklet/bookmarkletloader.min.js",
    "bookmarklet": "npm-run-all bookmarklet:*",
    "update:population:bwrift": "node tools/bwriftpop.js > data/pop-bwrift.csv",
    "update:population:iceberg": "node tools/icebergpop.js > data/pop-iceberg.csv",
    "update:population": "npm-run-all update:population:*",
    "population": "node build/processPopulation.js",
    "wisdom": "node build/processWisdom.js",
    "timestamp": "node build/processTimestamp.js",
    "build": "npm-run-all --parallel bookmarklet population wisdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsitu/MH-Tools.git"
  },
  "keywords": [
    "Mouse Hunt"
  ],
  "bugs": {
    "url": "https://github.com/tsitu/MH-Tools/issues"
  },
  "homepage": "https://tsitu.github.io/MH-Tools",
  "license": "Apache-2.0",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "combined-stream": "^1.0.5",
    "csvtojson": "^1.1.7",
    "delayed-stream": "^1.0.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.2",
    "request": "^2.83.0",
    "uglify-js": "^3.0.25"
  },
  "devDependencies": {
    "bluebird": "^3.5.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.6.0",
    "jacksmhtools-client": "0.0.1",
    "js-combinatorics": "^0.5.2",
    "json2csv": "^3.11.5",
    "lodash": "^4.17.4",
    "prettier": "1.9.1"
  }
}
