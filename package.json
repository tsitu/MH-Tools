{
  "name": "mh-tools",
  "version": "1.0.0",
  "description": "Assortment of MouseHunt utilities",
  "scripts": {
    "\n-- POPULATION DATA --": "",
    "pop:acolyte": "node data/pop acolyte-realm > data/pop-csv/acolyte-realm.csv",
    "pop:afterword": "node data/pop afterword-acres > data/pop-csv/afterword-acres.csv",
    "pop:balack": "node data/pop balacks-cove > data/pop-csv/balacks-cove.csv",
    "pop:beanstalk": "node data/pop bountiful-beanstalk > data/pop-csv/bountiful-beanstalk.csv",
    "pop:burroughs": "node data/pop burroughs-rift > data/pop-csv/burroughs-rift.csv",
    "!pop:bristle": "node data/pop bristle-woods-rift > data/pop-csv/bristle-woods-rift.csv",
    "pop:cantera": "node data/pop cantera-quarry > data/pop-csv/cantera-quarry.csv",
    "pop:catacombs": "node data/pop catacombs > data/pop-csv/catacombs.csv",
    "pop:cursed": "node data/pop cursed-city > data/pop-csv/cursed-city.csv",
    "pop:digby": "node data/pop town-of-digby > data/pop-csv/town-of-digby.csv",
    "pop:dd": "node data/pop draconic-depths > data/pop-csv/draconic-depths.csv",
    "pop:epilogue": "node data/pop epilogue-falls > data/pop-csv/epilogue-falls.csv",
    "pop:forbidden": "node data/pop forbidden-grove > data/pop-csv/forbidden-grove.csv",
    "pop:farm": "node data/pop foreword-farm > data/pop-csv/foreword-farm.csv",
    "pop:fi": "node data/pop floating-islands > data/pop-csv/floating-islands.csv",
    "pop:fort": "node data/pop fort-rox > data/pop-csv/fort-rox.csv",
    "pop:fungal": "node data/pop fungal-cavern > data/pop-csv/fungal-cavern.csv",
    "pop:furoma": "node data/pop furoma-rift > data/pop-csv/furoma-rift.csv",
    "!pop:ges": "node data/pop ges > data/pop-csv/ges.csv",
    "pop:geyser": "node data/pop queso-geyser > data/pop-csv/queso-geyser.csv",
    "pop:gnawnia": "node data/pop town-of-gnawnia > data/pop-csv/town-of-gnawnia.csv",
    "pop:gloomy": "node data/pop gloomy-greenwood > data/pop-csv/gloomy-greenwood.csv",
    "pop:gwh": "node data/pop great-winter-hunt > data/pop-csv/great-winter-hunt.csv",
    "pop:harbour": "node data/pop harbour > data/pop-csv/harbour.csv",
    "pop:iceberg": "node data/pop iceberg > data/pop-csv/iceberg.csv",
    "pop:jod": "node data/pop jungle-of-dread > data/pop-csv/jungle-of-dread.csv",
    "pop:lab": "node data/pop laboratory > data/pop-csv/laboratory.csv",
    "pop:laby": "node data/pop labyrinth > data/pop-csv/labyrinth.csv",
    "pop:living": "node data/pop living-garden > data/pop-csv/living-garden.csv",
    "pop:lost": "node data/pop lost-city > data/pop-csv/lost-city.csv",
    "pop:mopi": "node data/pop moussu-picchu > data/pop-csv/moussu-picchu.csv",
    "pop:mountain": "node data/pop mountain > data/pop-csv/mountain.csv",
    "pop:moz": "node data/pop mousoleum > data/pop-csv/mousoleum.csv",
    "pop:pond": "node data/pop prologue-pond > data/pop-csv/prologue-pond.csv",
    "pop:prickly": "node data/pop prickly-plains > data/pop-csv/prickly-plains.csv",
    "pop:queso": "node data/pop queso-river > data/pop-csv/queso-river.csv",
    "pop:ronza": "node data/pop ronza > data/pop-csv/ronza.csv",
    "pop:soso": "node data/pop school-of-sorcery > data/pop-csv/school-of-sorcery.csv",
    "pop:toc": "node data/pop table-of-contents > data/pop-csv/table-of-contents.csv",
    "pop:toxic": "node data/pop toxic-spill > data/pop-csv/toxic-spill.csv",
    "pop:vrift": "node data/pop valour-rift > data/pop-csv/valour-rift.csv",
    "pop:warpath": "node data/pop warpath > data/pop-csv/fiery-warpath.csv",
    "pop:wwrift": "node data/pop whisker-woods-rift > data/pop-csv/whisker-woods-rift.csv",
    "pop:zokor": "node data/pop zokor > data/pop-csv/zokor.csv",
    "pop:windmill": "node data/pop windmill > data/pop-csv/windmill.csv",
    "pop:zt": "node data/pop zugzwangs-tower > data/pop-csv/zugzwangs-tower.csv",
    "pop": "npm-run-all --parallel pop:*",
    "\n-- MINIFICATION --": "",
    "minify:bookmarklet": "uglifyjs-folder src/bookmarklet -yeo src/bookmarklet -p **/*.js,!**/*min.js",
    "minify:main": "uglifyjs-folder src/main -eo src/main/min -p **/*.js,!**/*min.js",
    "minify:utils": "uglifyjs-folder src/utils -eo src/utils/min -p **/*.js,!**/*min.js",
    "minify": "npm-run-all --parallel minify:*",
    "\n-- BUILD --": "",
    "puppeteer": "npm i puppeteer@18",
    "timestamp": "node build/process-timestamp.js",
    "sample": "node build/process-sample-size.js",
    "mouse-data": "node build/process-mouse-data.js",
    "population": "node build/process-population.js",
    "trap-data": "node build/process-trap-data.js",
    "build": "npm-run-all --parallel mouse-data population trap-data minify",
    "\n-- MISC --": "",
    "serve": "http-server -p 8000 -c-1 --cors",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsitu/MH-Tools.git"
  },
  "keywords": [
    "MouseHunt"
  ],
  "bugs": {
    "url": "https://github.com/tsitu/MH-Tools/issues"
  },
  "homepage": "https://tsitu.github.io/MH-Tools",
  "license": "Apache-2.0",
  "dependencies": {
    "combined-stream": "^1.0.5",
    "csvtojson": "^1.1.7",
    "delayed-stream": "^1.0.0",
    "jsdom": "^16.2.2",
    "mkdirp": "^0.5.1",
    "request": "^2.83.0",
    "uglify-js": "^3.19.3"
  },
  "devDependencies": {
    "@octokit/auth-action": "^2.0.4",
    "@octokit/rest": "^19.0.11",
    "bluebird": "^3.5.1",
    "csv": "^6.3.6",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.6.0",
    "http-server": "^0.11.1",
    "jacksmhtools-client": "^0.1.2",
    "js-combinatorics": "^0.5.2",
    "json2csv": "^3.11.5",
    "lodash": "^4.17.15",
    "mysql2": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.5.1",
    "prettier": "^1.18.2",
    "uglifyjs-folder": "^3.3.0"
  }
}
